<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="root"
	xmlns="http://javafx.com/javafx/10.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="apaintus.controllers.ZoomController">
	<children>
		<HBox>
			<Slider fx:id="zoomSlider" blockIncrement="10.0"
				majorTickUnit="10.0" max="150.0" min="50.0" minorTickCount="0"
				prefHeight="38.0" prefWidth="134.0" showTickLabels="true"
				showTickMarks="true" snapToTicks="true" translateY="5.0"
				value="100.0" />
			<Label fx:id="sliderLabel" text="100%" prefHeight="29.0" prefWidth="53.0" />
		</HBox>
	</children>
</AnchorPane>
